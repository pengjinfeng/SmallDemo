.container {
    background-color: #F2f2f2;
    
}
.homecontainer{
    width: 100%;
    align-items: flex-start;
}
.swiper_box {
    width: 100%;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;/*超出部分显示样式hidden 影藏 scroll 可滚动 */
}
view.text {
    width: 100%;
    display: flex;
    align-items: center;
    position: relative;
    padding: 10px 10px 10px 12px;
    font-size: 15px;
    color: #656565;
}

